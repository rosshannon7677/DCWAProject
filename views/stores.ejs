<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Stores</title>
</head>
<body>
    <h1>Stores</h1>
    <a href="/stores/add">Add Store</a>
    <table border="1">
        <thead>
            <tr>
                <th>SID</th>
                <th>Location</th>
                <th>Manager ID</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            <% stores.forEach(store => { %>
                <tr>
                    <td><%= store.sid %></td>
                    <td><%= store.location %></td>
                    <td><%= store.mgrid %></td>
                    <td>
                        <a href="/stores/update/<%= store.sid %>">Update</a>
                    </td>
                </tr>
            <% }); %>
        </tbody>
    </table>
    <a href="/">Home</a>
</body>
</html>